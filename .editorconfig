# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
# end_of_line = lf
insert_final_newline = true
indent_style = space

[*.css]
indent_size = 2
indent_style = space

[*.scss]
indent_size = 2
indent_style = space

[*.js]
indent_size = 4
indent_style = space

[*.php]
indent_size = 4
indent_style = space
